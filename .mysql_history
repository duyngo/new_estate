select t1.*,t2.name as developer_name,t3.code as states_code,t4.code as locations_code,t5.name as property_types_name from listings t1 left join companies t2 on ( t1.developer_id = t2.id ) left join states t3 on ( t1.states_id = t3.id ) left join locations t4 on ( t1.locations_id = t4.id ) left join property_types t5 on ( t1.property_types_id = t5.id ) where t1.code = 'kl-condmi' and t1.is_deleted = 0;
