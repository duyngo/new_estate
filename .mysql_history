CREATE TABLE `listings_for_urls` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `listings_id` int(10) NOT NULL,
  `name` varchar(128) NOT NULL,
  `code` varchar(64) NOT NULL,
  `companies_id` int(10) NOT NULL,
  `developer_id` int(10) NOT NULL,
  `states_id` int(10) NOT NULL,
  `groups_id` int(10) NOT NULL,
  `locations_id` int(10) NOT NULL,
  `property_types_id` varchar(256) NOT NULL,
  `prices_id` varchar(256) NOT NULL,
  `features_id` varchar(256) NOT NULL,
  `completion_years_id` varchar(256) NOT NULL,
  `bedrooms_id` varchar(256) NOT NULL,
  `sizes_id` varchar(256) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=10001 DEFAULT CHARSET=utf8 COMMENT='';
CREATE TABLE `listings_for_urls` (   `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',   `listings_id` int(10) NOT NULL,   `name` varchar(128) NOT NULL,   `code` varchar(64) NOT NULL,   `companies_id` int(10) NOT NULL,   `developer_id` int(10) NOT NULL,   `states_id` int(10) NOT NULL,   `groups_id` int(10) NOT NULL,   `locations_id` int(10) NOT NULL,   `property_types_id` varchar(256) NOT NULL,   `prices_id` varchar(256) NOT NULL,   `features_id` varchar(256) NOT NULL,   `completion_years_id` varchar(256) NOT NULL,   `bedrooms_id` varchar(256) NOT NULL,   `sizes_id` varchar(256) NOT NULL,   PRIMARY KEY (`id`) ) ENGINE=MyISAM AUTO_INCREMENT=10001 DEFAULT CHARSET=utf8 COMMENT='';
