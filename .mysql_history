CREATE TABLE `listings_urls` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',
  `listings_id` int(10) NOT NULL,
  `urls_id` int(10) NOT NULL,
  `is_deleted` tinyint(1) NOT NULL DEFAULT '0' COMMENT '論理削除フラグ',
  `created` datetime NOT NULL COMMENT '登録日時',
  `created_by` int(10) unsigned NOT NULL COMMENT '登録者ID',
  `modified` datetime NOT NULL COMMENT '更新日時',
  `modified_by` int(10) unsigned NOT NULL COMMENT '更新者ID',
  PRIMARY KEY (`id`)
) ENGINE=MyISAM AUTO_INCREMENT=10001 DEFAULT CHARSET=utf8 COMMENT='listings_urls';
CREATE TABLE `listings_urls` (   `id` int(10) unsigned NOT NULL AUTO_INCREMENT COMMENT 'ID',   `listings_id` int(10) NOT NULL,   `urls_id` int(10) NOT NULL,   `is_deleted` tinyint(1) NOT NULL DEFAULT '0' COMMENT '論理削除フラグ',   `created` datetime NOT NULL COMMENT '登録日時',   `created_by` int(10) unsigned NOT NULL COMMENT '登録者ID',   `modified` datetime NOT NULL COMMENT '更新日時',   `modified_by` int(10) unsigned NOT NULL COMMENT '更新者ID',   PRIMARY KEY (`id`) ) ENGINE=MyISAM AUTO_INCREMENT=10001 DEFAULT CHARSET=utf8 COMMENT='listings_urls';
