select t1.*,t2.name as developer_name,t2.logo_image_path,t3.name as states_name,t3.code as states_code,t4.name as groups_name,t4.code as groups_code,t5.name as locations_name,t5.code as locations_code from listings t1 left join companies t2 on ( t1.developer_id = t2.id ) left join states t3 on ( t1.states_id = t3.id ) left join groups t4 on ( t1.groups_id = t4.id ) left join locations t5 on ( t1.locations_id = t5.id ) where t1.is_deleted = 0 and t1.status = 'current' and t1.states_id = 10002 order t1.monthly_enquiry_limit desc,t1.achievement_rate = 0 asc,t1.achievement_rate asc,t1.search_rank desc,t1.modified desc;
select t1.*,t2.name as developer_name,t2.logo_image_path,t3.name as states_name,t3.code as states_code,t4.name as groups_name,t4.code as groups_code,t5.name as locations_name,t5.code as locations_code from listings t1 left join companies t2 on ( t1.developer_id = t2.id ) left join states t3 on ( t1.states_id = t3.id ) left join groups t4 on ( t1.groups_id = t4.id ) left join locations t5 on ( t1.locations_id = t5.id ) where t1.is_deleted = 0 and t1.status = 'current' and t1.states_id = 10002 order t1.monthly_enquiry_limit desc;
\q
